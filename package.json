{
        "name": "projectpages",
        "version": "1.8.1",
        "repository": {
                "type": "git",
                "directory": "https://github.com/SAPMarco/SAPMarco.github.io"
        },
        "homepage": "https://sapmarco.github.io/",
        "engines": {
                "node": "12.x.x"
        },
        "scripts": {
                "start": "ui5 serve -o index.html",
                "start-build": "ui5 serve -o index.html --config ui5-dist.yaml",
                "serve-unit": "ui5 serve --port 8081 --open /test/unit/unitTests.qunit.html",
                "test": "npm run lint && npm run karma",
                "karma-ci": "karma start karma-ci.conf.js",
                "karma": "rimraf coverage && npm run karma-ci",
                "lint": "eslint webapp",
                "build": "ui5 build self-contained --all --dest ./docs",
                "build-no-debug": "ui5 build self-contained --all --exclude-task=createDebugFiles --clean-dest true --dest ./docs"
        },
        "dependencies": {
                "@highlightjs/cdn-assets": "^10.2.0",
                "marked": "^1.1.1",
                "shx": "^0.3.2"
        },
        "devDependencies": {
                "@openui5/ts-types": "^1.60.5",
                "@ui5/cli": "^2.4.4",
                "eslint": "^7.9.0",
                "karma": "^5.2.3",
                "karma-chrome-launcher": "^3.1.0",
                "karma-coverage": "^2.0.3",
                "karma-ui5": "^2.3.0",
                "ui5-middleware-livereload": "^0.4.2",
                "ui5-task-minifier": "^1.0.2"
        },
        "ui5": {
                "dependencies": [
                        "@highlightjs/cdn-assets",
                        "marked",
                        "ui5-middleware-livereload",
                        "ui5-task-minifier"
                ]
        }
}
