{
        "name": "projectpages",
        "version": "0.1.5",
        "repository": {
                "type": "git",
                "directory": "https://github.com/SAPMarco/github_pages"
        },
        "homepage": "https://sapmarco.github.io/",
        "engines": {
                "node": "12.x.x"
        },
        "scripts": {
                "start": "ui5 serve -o index.html",
                "serve": "ui5 serve",
                "test": "npm run lint && npm run karma",
                "karma-ci": "karma start karma-ci.conf.js",
                "karma": "rimraf coverage && npm run karma-ci",
                "lint": "eslint webapp",
                "build:ui": "ui5 build --clean-dest --dest approuter/webapp ",
                "build": "ui5 build self-contained --all --exclude-task=createDebugFiles --clean-dest --dest ./docs"
        },
        "dependencies": {
                "@openui5/sap.m": "^1.73.1",
                "@openui5/sap.ui.core": "^1.74.0",
                "@openui5/sap.ui.layout": "^1.74.0",
                "@openui5/themelib_sap_belize": "^1.74.0",
                "@openui5/themelib_sap_fiori_3": "^1.74.0",
                "shx": "^0.3.2"
        },
        "devDependencies": {
                "@ui5/cli": "^1.14.0",
                "eslint": "^6.7.2",
                "karma": "^4.4.1",
                "karma-chrome-launcher": "^3.1.0",
                "karma-coverage": "^2.0.1",
                "karma-ui5": "^1.1.0",
                "ui5-middleware-livereload": "^0.1.5"
        },
        "ui5": {
                "dependencies": [
                        "@openui5/sap.m",
                        "@openui5/sap.ui.core",
                        "@openui5/sap.ui.layout",
                        "@openui5/themelib_sap_belize",
                        "@openui5/themelib_sap_fiori_3",
                        "ui5-middleware-livereload"
                ]
        }
}
