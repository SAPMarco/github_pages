{
        "name": "projectpages",
        "version": "1.2.2",
        "repository": {
                "type": "git",
                "directory": "https://github.com/SAPMarco/SAPMarco.github.io"
        },
        "homepage": "https://sapmarco.github.io/",
        "engines": {
                "node": "12.x.x"
        },
        "scripts": {
                "start": "ui5 serve -o index.html",
                "serve": "ui5 serve",
                "test": "npm run lint && npm run karma",
                "karma-ci": "karma start karma-ci.conf.js",
                "karma": "rimraf coverage && npm run karma-ci",
                "lint": "eslint webapp",
                "build:ui": "ui5 build --clean-dest --dest approuter/webapp ",
                "build": "ui5 build self-contained --all --exclude-task=createDebugFiles --clean-dest --dest ./docs"
        },
        "dependencies": {
                "marked": "^1.1.0",
                "npm-check-updates": "^7.0.1",
                "shx": "^0.3.2"
        },
        "devDependencies": {
                "@openui5/ts-types": "^1.60.5",
                "@ui5/cli": "^2.2.6",
                "eslint": "^7.3.1",
                "karma": "^5.1.0",
                "karma-chrome-launcher": "^3.1.0",
                "karma-coverage": "^2.0.2",
                "karma-ui5": "^2.2.0",
                "ui5-middleware-livereload": "^0.3.0"
        },
        "ui5": {
                "dependencies": [
                        "ui5-middleware-livereload"
                ]
        }
}
